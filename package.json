{
  "name": "openserv-sdk-intro",
  "version": "1.0.0",
  "description": "Comprehensive introduction and testing suite for OpenServ TypeScript SDK",
  "main": "leaderboard-boosted.ts",
  "scripts": {
    "build": "echo 'No build needed'",
    "start": "npx ts-node leaderboard-boosted.ts",
    "test": "npm run test:capabilities && npm run test:tasks && npm run test:chat",
    "test:capabilities": "ts-node -e \"import('./leaderboard-boosted').then(m => m.testBasicCapabilities())\"",
    "test:tasks": "ts-node -e \"import('./leaderboard-boosted').then(m => m.demonstrateTaskManagement())\"",
    "test:chat": "ts-node -e \"import('./leaderboard-boosted').then(m => m.demonstrateChatCommunication())\"",
    "test:files": "ts-node -e \"import('./leaderboard-boosted').then(m => m.demonstrateFileOperations())\"",
    "test:integrations": "ts-node -e \"import('./leaderboard-boosted').then(m => m.demonstrateIntegrationManagement())\"",
    "test:advanced": "ts-node -e \"import('./leaderboard-boosted').then(m => m.demonstrateAdvancedFeatures())\"",
    "test:all": "ts-node -e \"import('./leaderboard-boosted').then(m => m.runComprehensiveTests())\"",
    "dev": "ts-node --watch leaderboard-boosted.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "openserv",
    "ai",
    "agent",
    "typescript",
    "sdk",
    "framework",
    "autonomous",
    "cognitive"
  ],
  "author": "OpenServ Labs",
  "license": "MIT",
  "dependencies": {
    "@openserv-labs/sdk": "^1.6.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.516.0",
    "node-telegram-bot-api": "^0.66.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/node": "^20.10.0",
    "@types/node-telegram-bot-api": "^0.64.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openserv-labs/openserv-sdk-intro.git"
  },
  "bugs": {
    "url": "https://github.com/openserv-labs/openserv-sdk-intro/issues"
  },
  "homepage": "https://github.com/openserv-labs/openserv-sdk-intro#readme"
}
